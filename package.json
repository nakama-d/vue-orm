{
  "name": "vue-orm",
  "version": "1.0.0",
  "description": "A full native Object-Relational Mapping Store to the Vue 3",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "scripts": {
    "dev": "ts-node ./__tests__/index.ts",
    "doc": "typedoc src/index.ts",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "prettier **/*.{ts,tsx,js,jsx,json} --check",
    "test:unit": "jest",
    "lint": "prettier **/*.{ts,tsx,js,jsx,json} --write",
    "build": "bili",
    "prepublishOnly": "npm run doc && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakama-d/vue-orm.git"
  },
  "author": "Nakama d",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nakama-d/vue-orm/issues"
  },
  "homepage": "https://github.com/nakama-d/vue-orm#readme",
  "dependencies": {
    "@nakama.d/normalizr-ts": "^1.0.1",
    "clone": "^2.1.2",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/shortid": "0.0.29",
    "bili": "^5.0.5",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "prettier": "^2.1.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.3",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "singleQuote": true,
    "printWidth": 80,
    "arrowParens": "always",
    "semi": false
  }
}
